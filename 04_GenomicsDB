#joint genotyping on GenomicsDB workspace created with GenomicsDBImport
#in this case /merge was the workspaces directory
#$1 = reference fasta
gatk --java-options "-Xmx4g" GenotypeGVCFs \
   -R $1 \
   -V gendb://merge \
   -O Ptr_all-isolates_unfiltered.vcf.gz \
   --tmp-dir /isilon/lethbridge-rdc/users/gourlier/vcf/tmp
