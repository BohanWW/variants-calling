#merge GVCFs from multiple samples to unify SNP quality scores
#should make this universal later
gatk --java-options "-Xmx4g -Xms4g" GenomicsDBImport \
      -V gvcf/331-2.g.vcf.gz \
      -V gvcf/86-124.g.vcf.gz \
      -V gvcf/90-2.g.vcf.gz \
      -V gvcf/92-171.g.vcf.gz \
      -V gvcf/AB88-2.g.vcf.gz \
