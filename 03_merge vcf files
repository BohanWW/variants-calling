      -V gvcf/SW15-1.g.vcf.gz \
      -V gvcf/SW2-1.g.vcf.gz \
      -V gvcf/SW20-7.g.vcf.gz \
      -V gvcf/SW21-1.g.vcf.gz \
      -V gvcf/SW21-5.g.vcf.gz \
      -V gvcf/SW21-7.g.vcf.gz \
      -V gvcf/SW21-8.g.vcf.gz \
      -V gvcf/SW7-5.g.vcf.gz \
