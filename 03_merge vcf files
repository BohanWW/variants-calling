      -V gvcf/Alg3-24.g.vcf.gz \
      -V gvcf/Alg4x-1.g.vcf.gz \
      -V gvcf/AlgH1.g.vcf.gz \
      -V gvcf/D308.g.vcf.gz \
      -V gvcf/G9-4.g.vcf.gz \
      -V gvcf/I17-2.g.vcf.gz \
      -V gvcf/I33-1.g.vcf.gz \
