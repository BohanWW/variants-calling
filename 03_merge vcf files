      -V gvcf/T176-2.g.vcf.gz \
      -V gvcf/T181-1.g.vcf.gz \
      --genomicsdb-workspace-path /isilon/lethbridge-rdc/users/gourlier/vcf/M4_VCF/merge_a \
      --tmp-dir /isilon/lethbridge-rdc/users/gourlier/vcf/M4_vcf/tmp_a \
      --interval-padding 100 \
      -L gvcf/331-2.g.vcf.gz \
